[user]
	name = Lamengao
	email = lamengao@gmail.com
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
	oneline = log --pretty=oneline --since='2 days ago'
	onelog = log -p -1
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[color]
	status = auto
	branch = auto
	ui = auto
[core]
	quotepath = false
[push]
	default = simple
